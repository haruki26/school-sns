{
  "name": "backend",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/devServer.ts",
    "build": "tsc",
    "start": "node dist/src/routes/index.js",
    "lint": "eslint",
    "format": "prettier .",
    "check": "prettier --write . && eslint --fix",
    "prisma:mig": "dotenv -- sh -c 'pnpm prisma migrate dev --schema=./prisma/${PROVIDER_TYPE}/schema.prisma'",
    "prisma:gen": "dotenv -- sh -c 'pnpm prisma generate --schema=./prisma/${PROVIDER_TYPE:-sqlserver}/schema.prisma'",
    "prisma:seed": "prisma db seed",
    "prisma:std": "dotenv -- sh -c 'pnpm prisma studio --schema=./prisma/${PROVIDER_TYPE:-sqlserver}/schema.prisma --port 51212'",
    "test": "vitest run",
    "test:pre": "DATABASE_URL=\"file:./database/test.db\" npx prisma db push --accept-data-loss",
    "test:cov": "vitest run --coverage"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.6",
    "@hono/oauth-providers": "^0.8.5",
    "@hono/standard-validator": "^0.2.1",
    "@hono/swagger-ui": "^0.5.2",
    "@langchain/core": "^1.1.8",
    "@langchain/google-genai": "^2.1.3",
    "@praha/byethrow": "^0.9.0",
    "@prisma/client": "^5.22.0",
    "argon2": "^0.44.0",
    "dotenv": "^17.2.3",
    "hono": "^4.11.0",
    "hono-openapi": "^1.1.2",
    "ioredis": "^5.9.2",
    "langchain": "^1.2.3",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.2.0",
    "@hono/eslint-config": "^2.0.3",
    "@types/better-sqlite3": "^7.6.13",
    "@types/ioredis-mock": "^8.2.6",
    "@types/node": "^20.19.26",
    "@vitest/coverage-v8": "catalog:",
    "dotenv-cli": "^11.0.0",
    "eslint": "catalog:",
    "ioredis-mock": "^8.13.1",
    "prettier": "catalog:",
    "prisma": "^5.22.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typescript": "catalog:",
    "vite-tsconfig-paths": "catalog:",
    "vitest": "catalog:"
  },
  "prisma": {
    "seed": "tsx src/lib/seed.ts"
  }
}
